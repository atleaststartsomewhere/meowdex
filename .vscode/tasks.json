{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "maui: build windows debug",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Meowdex.App/Meowdex.App.csproj",
        "-f",
        "net10.0-windows10.0.19041.0"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "desktop: build debug",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/Meowdex.Desktop/Meowdex.Desktop.csproj"
      ],
      "group": "build",
      "problemMatcher": "$msCompile"
    }
  ]
}
