<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Name="Root"
             x:Class="Meowdex.Desktop.Views.Badges.GenderBadgeView">
  <Image Classes="identity-icon"
         Width="16"
         Height="16"
         HorizontalAlignment="Center"
         VerticalAlignment="Center"
         Source="{Binding Gender, ElementName=Root, Converter={StaticResource GenderIconConverter}}" />
</UserControl>
